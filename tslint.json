{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "variable-name": [
      false
    ],
    "no-console": [
      false
    ],
    "no-shadowed-variable": [
      false
    ],
    "one-variable-per-declaration": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "ban-types": [
      false
    ],
    "interface-name": [
      false
    ]
  },
  "rulesDirectory": []
}
